module test_main;
import stdio local;

const char[] Data1 @(embed="files/data1.dat");
const char[] File2 @(embed="files/file2.txt");

fn void hex_dump(const u8* data, u32 len) {
    for (u32 i=0; i<len; i++) {
        printf("%02X ", data[i]);
        if (i%16 == 15) printf("\n");
    }
    if (len%16 != 15) printf("\n");
}

public fn i32 main(i32 argc, char** argv) {
    printf("FILE1: size %d\n", elemsof(Data1));
    hex_dump(Data1, elemsof(Data1));

    printf("FILE2: size %d\n", elemsof(File2));
    printf("%s\n", File2);

	return 0;
}

